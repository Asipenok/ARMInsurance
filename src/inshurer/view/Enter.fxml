<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="inshurer.view.EnterController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <image>
            <Image url="@enter-texture.jpg" />
         </image>
      </ImageView>
      <Pane stylesheets="@button1.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="CENTER" layoutX="60.0" layoutY="50.0" prefHeight="300.0" prefWidth="500.0" spacing="15.0">
               <children>
                  <Label text="USERNAME" textFill="#f23d22">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <font>
                        <Font name="Bookman Old Style Bold Italic" size="30.0" />
                     </font>
                  </Label>
                  <TextField fx:id="fieldLogin" prefHeight="50.0" promptText="Enter Login">
                     <VBox.margin>
                        <Insets bottom="20.0" />
                     </VBox.margin>
                  </TextField>
                  <Label text="PASSWORD" textFill="#ff0b0b">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <font>
                        <Font name="Bookman Old Style Bold Italic" size="30.0" />
                     </font>
                  </Label>
                  <PasswordField fx:id="fieldPassword" prefHeight="50.0" prefWidth="300.0" promptText="Enter password" />
                  <Button fx:id="btnEnter" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#clickEnter" prefHeight="50.0" prefWidth="150.0" text="ENTER">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets top="25.0" />
                     </VBox.margin>
                     <rotationAxis>
                        <Point3D />
                     </rotationAxis>
                  </Button>
               </children>
               <padding>
                  <Insets left="100.0" right="100.0" />
               </padding>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </VBox>
         </children>
      </Pane>
   </children>
</AnchorPane>
