<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="900.0" prefWidth="900.0" stylesheets="@JMetroDarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="inshurer.view.CarController">
   <children>
      <GridPane AnchorPane.bottomAnchor="200.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="200.0" AnchorPane.topAnchor="200.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="250.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="490.0" minWidth="10.0" prefWidth="267.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="490.0" minWidth="10.0" prefWidth="137.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" text="Автомобиль" textAlignment="JUSTIFY" GridPane.columnSpan="3">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <Label text="Марка" GridPane.rowIndex="2" />
            <Label text="Модель" GridPane.rowIndex="3" />
            <Label text="VIN номер кузова" GridPane.rowIndex="4" />
            <Label text="Регистрационный номер" GridPane.rowIndex="5" />
            <Label text="Год выпуска" GridPane.rowIndex="6" />
            <Label text="Стоимость" GridPane.rowIndex="7" />
            <DatePicker fx:id="yearCar" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="6" />
            <TextField fx:id="fieldVIN" prefHeight="31.0" prefWidth="209.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="fieldNumberCar" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField fx:id="fieldCoastCar" GridPane.columnIndex="1" GridPane.rowIndex="7" />
            <Button fx:id="btn_searchCarNumber" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#clickSearchByNumber" text="Искать" GridPane.columnIndex="2" GridPane.rowIndex="5" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnSpan="3" GridPane.rowIndex="8">
               <children>
                  <HBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Button fx:id="btn_Clear" mnemonicParsing="false" onAction="#clickClear" prefWidth="80.0" text="Clear" />
                        <Button fx:id="btn_Edit" mnemonicParsing="false" onAction="#onClickEdit" prefWidth="80.0" text="Edit" />
                        <Button fx:id="btn_saveCar" mnemonicParsing="false" onAction="#clickSaveCar" prefWidth="80.0" text="Save" />
                     </children>
                  </HBox>
               </children>
            </HBox>
            <TextField fx:id="fieldBrandCar" prefHeight="31.0" prefWidth="332.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="2" />
            <TextField fx:id="fieldModelCar" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="3" />
            <ComboBox fx:id="boxCurrencyCar" prefHeight="31.0" prefWidth="217.0" GridPane.columnIndex="2" GridPane.rowIndex="7" />
            <Button fx:id="btn_searchCarID" mnemonicParsing="false" onAction="#clickSearchByVIN" prefHeight="31.0" prefWidth="112.0" text="Искать" GridPane.columnIndex="2" GridPane.rowIndex="4" />
            <Label text="Тип ТС" GridPane.rowIndex="1" />
            <ComboBox fx:id="boxTypeCar" prefHeight="31.0" prefWidth="318.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
